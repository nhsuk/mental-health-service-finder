<h3 class="results__name">{{ result.OrganisationNameHighlight | safe }}</h3>

{% if result.fullAddress %}
  <p class="results__address results__address-lines">
  {{ result.fullAddress | safe }}
  </p>
{% endif %}

<p><a href="{{ siteRoot }}/results?type=iapt&query={{ result.CCG[0] }}&gpquery={{ query | urlencode }}&gpname={{ result.OrganisationName | urlencode }}">This is my GP</a></p>
